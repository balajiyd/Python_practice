y = input("Enter the DD/MM/YYYY: ")
k=y.split('/')
year = k[2]

j = int(year)
if(j%4 == 0 and j%100 != 0) or (j%400 == 0):

    print("The year ",y,"is a leap year ")
    print("Next Anniversary: ",k[0],'/',k[1],'/',j+1)

else:

    print("the given year",y,"is not leap year")
    print("Next Anniversary: ", k[0],'/',k[1],'/',j-1)
